{
  "name": "@movefuns/move-js",
  "version": "0.7.2",
  "description": "Javascript version of the Move language compiler, supports compiling Move code into Move bytecode in the browser.",
  "keywords": [
    "move",
    "rust",
    "sui",
    "smart contracts",
    "facebook",
    "diem",
    "blockchain"
  ],
  "files": [
    "dist",
    "src",
    "pkg",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/movefuns/move-js"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "iife": "dist/index.iife.js",
  "types": "dist/types/index.d.ts",
  "browserslist": "> 0.25%, not dead",
  "scripts": {
    "build:rust": "cargo +nightly build -Zbuild-std=std,panic_abort --target=wasm32-wasip1 --release && cp ./target/wasm32-wasip1/release/move-web.wasm ./pkg/move_bg.wasm",
    "build:js": "rollup -c --environment BUILD:production",
    "build": "run-s clean build:rust build:js",
    "test:integration": "babel-node --presets @babel/preset-env -- ./node_modules/karma/bin/karma start karma.conf.js",
    "test": "run-s build test:integration",
    "lint": "run-s lint:ts:tslint lint:ts:preetier",
    "lint:ts:tslint": "tslint --project tsconfig.json -t codeFrame 'pkg/**/*.ts'",
    "lint:ts:preetier": "prettier --write pkg/**/*.ts",
    "lint:rust": "cargo fmt",
    "clean": "rimraf dist && rimraf pkg/*.wasm",
    "release": "npm publish --access public --tag latest"
  },
  "author": "movefuns",
  "license": "SEE LICENSE IN ./LICENSE",
  "peerDependencies": {
    "rollup": "2.x.x"
  },
  "devDependencies": {
    "@allex/rollup-plugin-node-globals": "^0.0.2",
    "@babel/core": "^7.29.0",
    "@babel/node": "^7.29.0",
    "@babel/plugin-transform-object-rest-spread": "^7.25.9",
    "@babel/preset-env": "^7.29.0",
    "@babel/register": "^7.28.6",
    "@crokita/rollup-plugin-node-builtins": "^2.1.3",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-url": "^8.0.2",
    "@rollup/plugin-wasm": "^6.2.2",
    "@types/jest": "^27.5.2",
    "@types/karma": "^6.3.9",
    "@types/mocha": "^10.0.10",
    "@types/node": "^14.18.63",
    "@wasm-tool/wasm-pack-plugin": "^1.7.0",
    "@zip.js/zip.js": "^2.8.16",
    "babel-node": "^0.0.1-security",
    "cross-env": "^10.1.0",
    "eslint": "^9.26.0",
    "husky": "^9.1.7",
    "jasmine-core": "^6.0.1",
    "jest": "^29.7.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-rollup2-preprocessor": "^1.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.2",
    "rollup": "^2.79.2",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "@rollup/plugin-terser": "^0.4.4",
    "rollup-plugin-typescript2": "^0.36.0",
    "tslib": "^2.8.1",
    "tslint": "^6.1.3",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "@swc/helpers": "^0.5.18",
    "@wasmer/wasi": "^1.2.2",
    "@wasmer/wasm-transformer": "^0.12.0",
    "@wasmer/wasmfs": "^0.12.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "resolutions": {
    "bl": "^4.1.0",
    "semver": "^7.6.3",
    "elliptic": "^6.5.7",
    "glob": "^10.4.5",
    "inflight": "^1.0.6",
    "rimraf": "^5.0.10"
  }
}
